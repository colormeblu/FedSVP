{
  "base_config": "configs/pacs_fedsvp.json",
  "experiment_name": "pacs_fedsvp_loo_ablation",
  "outdir": "outputs",
  "grid": {
    "seed": [0, 42],
    "dataset.target_domain": ["art", "cartoon", "photo", "sketch"]
  },
  "ablations": {
    "standard_prompt_fedavg": {
      "algorithm.prompt_mode": "input",
      "algorithm.server_agg": "fedavg",
      "algorithm.use_gca": false
    },
    "msvp_only": {
      "algorithm.prompt_mode": "msvp",
      "algorithm.server_agg": "fedavg",
      "algorithm.use_gca": false
    },
    "msvp_gca": {
      "algorithm.prompt_mode": "msvp",
      "algorithm.server_agg": "fedavg",
      "algorithm.use_gca": true
    },
    "sdwa_only": {
      "algorithm.prompt_mode": "input",
      "algorithm.server_agg": "sdwa",
      "algorithm.use_gca": false
    },
    "fedsvp_full": {
      "algorithm.prompt_mode": "msvp",
      "algorithm.server_agg": "sdwa",
      "algorithm.use_gca": true
    }
  }
}
