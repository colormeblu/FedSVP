{
  "experiment": {
    "name": "pacs_fedavg",
    "outdir": "outputs"
  },
  "seed": 42,
  "num_workers": 4,
  "dataset": {
    "name": "pacs",
    "root": "/home/dengyu/dataset",
    "protocol": "loo",
    "target_domain": "sketch",
    "val_ratio": 0.1,
    "image_size": 224,
    "transforms": "clip"
  },
  "algorithm": {
    "name": "fedavg",
    "rounds": 2,
    "local_epochs": 1,
    "batch_size": 32,
    "lr": 0.001,
    "momentum": 0.9,
    "wd": 0.0005,
    "log_every": 1,
    "tqdm": false
  },
  "model": {
    "backbone": "open_clip",
    "open_clip": {
      "name": "ViT-B-16",
      "pretrained": "openai"
    }
  }
}